{
  "name": "zesty-website",
  "author": "Zesty.io Platform Inc.",
  "email": "marketing@zesty.io",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "build-start": "npm run build && npm start ",
    "prettier:fix": "prettier './src/**/*.js' --write",
    "clear-all": "rimraf ./node_modules ./.next",
    "sync": "node src/lib/sync.js",
    "docker-build": "docker build .",
    "gcloud-build": "gcloud builds submit --tag gcr.io/zesty-dev/zesty-website:1.0.0  --project zesty-dev",
    "gcloud-deploy": "gcloud run deploy zesty-website --image=gcr.io/zesty-dev/zesty-website:1.0.0 --platform managed --port 3000 --project zesty-dev --allow-unauthenticated --min-instances 1 --region us-central1",
    "deploy-stage": "npm run gcloud-build && npm run gcloud-deploy",
    "prepare:dev": "chmod ug+x .husky/*",
    "prepare": "husky install && npm run prepare:dev",
    "test": "jest --forceExit",
    "dev-test": "jest --watch --silent",
    "fix-lint": "eslint . --ext=js --fix",
    "commit": " git add . && git commit -m ' '",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint ",
    "cy:install": "cypress install",
    "cy:run": "cypress run --headless --browser chrome",
    "cy:open": "cypress open --browser chrome",
    "cy:test": "start-cy:run 3000",
    "test:e2e:ci": "start-server-and-test dev http://test.zesty.io:3000/ cy:run",
    "test:e2e:ci:prod": "start-server-and-test build-start http://test.zesty.io:3000/ cy:run"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/lab": "^5.0.0-alpha.97",
    "@mui/material": "^5.6.1",
    "@mui/x-data-grid": "^5.15.2",
    "@sweetalert2/theme-material-ui": "^5.0.11",
    "@zesty-io/live-editor": "^2.0.22",
    "@zesty-io/material": "^0.2.5",
    "@zesty-io/react-autolayout": "^1.0.0-beta.6",
    "aos": "^2.3.4",
    "axios": "^0.27.2",
    "cookies-next": "^2.0.4",
    "dayjs": "^1.11.4",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "himalaya": "^1.1.0",
    "jarallax": "^1.12.8",
    "markdown-it": "^13.0.1",
    "markdown-to-jsx": "^7.1.7",
    "next": "^12.2.5",
    "node-fetch": "^2.6.7",
    "notistack": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-confetti": "^6.1.0",
    "react-dom": "^17.0.1",
    "react-fast-marquee": "^1.3.4",
    "react-image-lightbox": "^5.1.4",
    "react-intersection-observer": "^9.4.0",
    "react-lazy-load-image-component": "^1.5.4",
    "react-player": "^2.10.0",
    "react-slick": "^0.28.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.7",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^11.4.24",
    "sweetalert2-react-content": "^4.2.0",
    "swiper": "^8.3.2",
    "tape-async": "^2.3.0",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "colortape": "^0.1.2",
    "cross-env": "^7.0.2",
    "cypress": "^7.4.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "2.7.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "react-virtualized": {
      "react": "17.0.2",
      "react-dom": "17.0.2"
    },
    "cypress-plugin-snapshots": {
      "cypress": "7.7.0"
    }
  }
}
